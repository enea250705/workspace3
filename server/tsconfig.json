{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "strictFunctionTypes": false,
    "outDir": "../dist",
    "skipLibCheck": true,
    "types": ["node", "express"]
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
} 